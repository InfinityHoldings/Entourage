<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
 
 <hibernate-mapping> 
     <class name="models.Event" table="events">
     <meta attribute="class description"> 
         This class contains all info pertaining to an event 
         </meta>
         
     <id name="evnt_id" type="int" column="evnt_id"> 
         <generator class="native" /> 
         </id>
         
     <property name="name" type="string" column="name" /> 
     <property name="privacy" type="string" column="privacy" /> 
     <property name="type" type="string" column="type" /> 
     <property name="host" type="string" column="host" /> 
     <property name="address1" column="address1" type="string"/>
     <property name="ent_usr_uid" column="ent_usr_uid" type="int" /> 
     
     
     
     </class>
     </hibernate-mapping>